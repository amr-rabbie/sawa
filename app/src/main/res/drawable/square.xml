<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp"/>
            <size android:height="35dp" android:width="35dp"/>
            <corners
                android:bottomLeftRadius="1.5dp"
                android:bottomRightRadius="1.5dp"
                android:topLeftRadius="1.5dp"
                android:topRightRadius="1.5dp" />
            <stroke android:color="@color/MainBackground" />
            <solid android:color="@color/MainBackground" />
        </shape>
    </item>
    <item android:drawable="@mipmap/ic_cart_outline" />
</layer-list>